{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "ProductImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ProductDescription": {
      "type": "string"
    },
    "stock": {
      "type": "integer"
    },
    "MaximumPurchaseLimit": {
      "type": "integer"
    }
  }
}
